#include <stdio.h>
#include <string.h>
#include <stdbool.h>

// Function to swap two characters
void swap(char *a, char *b) {
    char temp = *a;
    *a = *b;
    *b = temp;
}

// Function to check if swapping will create duplicate
bool shouldSwap(char str[], int start, int curr) {
    for (int i = start; i < curr; i++) {
        if (str[i] == str[curr]) {
            return false;
        }
    }
    return true;
}

// Recursive function to generate permutations
void permute(char str[], int l, int r) {
    if (l == r) {
        printf("%s\n", str);
        return;
    }

    for (int i = l; i <= r; i++) {
        // Skip duplicate characters
        if (shouldSwap(str, l, i)) {
            swap(&str[l], &str[i]);
            permute(str, l + 1, r);
            swap(&str[l], &str[i]); // backtrack
        }
    }
}

int main() {
    char str[20];
    printf("Enter a string: ");
    scanf("%s", str);

    int n = strlen(str);
    printf("Unique permutations are:\n");
    permute(str, 0, n - 1);

    return 0;
}
